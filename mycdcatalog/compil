g++ -fno-exceptions -fno-check-new -Wall -Wpointer-arith -Wmissing-prototypes -Wwrite-strings -Wno-long-long -Wnon-virtual-dtor -fno-builtin -Wall -fexceptions -I/usr/include/mysql -o thread thread.o -L/usr/lib/mysql -L/usr/local/lib -L/usr/lib/qt-2.3.0/lib -L/usr/lib  /usr/lib/libksycoca.so /usr/lib/libkio.so -L/usr/lib/gcc-lib/i386-redhat-linux/2.96 -L/usr/lib/gcc-lib/i386-redhat-linux/2.96/../../.. -lutil /usr/lib/libkdeui.so -ldl /usr/lib/libDCOP.so -lstdc++ -lc -lgcc -lqt-mt -lpng -lz -lm /usr/lib/libjpeg.so -Wl,--rpath -Wl,/usr/local/lib -Wl,--rpath -Wl,/usr/local/lib -Wl,--rpath -Wl,/usr/lib -Wl,--rpath -Wl,/usr/lib/qt-2.3.0/lib -Wl,--rpath

./thread

